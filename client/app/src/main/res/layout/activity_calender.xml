<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".calender_">

    <ImageView
        android:id="@+id/menu_container"
        android:layout_width="363dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/menu_container" />

    <Button
        android:id="@+id/trips_button"
        style="?attr/materialIconButtonStyle"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="13dp"
        android:background="@drawable/plane"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/menu_container"
        app:layout_constraintStart_toStartOf="@+id/menu_container"
        app:layout_constraintTop_toTopOf="@+id/menu_container"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/list_button"
        style="?attr/materialIconButtonStyle"
        android:layout_width="64dp"
        android:layout_height="62dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/note"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/menu_container"
        app:layout_constraintStart_toEndOf="@+id/trips_button"
        app:layout_constraintTop_toTopOf="@+id/menu_container"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/calendar_button"
        style="?attr/materialIconButtonStyle"
        android:layout_width="43dp"
        android:layout_height="43dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="13dp"
        android:background="@drawable/calendar"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/menu_container"
        app:layout_constraintEnd_toStartOf="@+id/profile_button"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/list_button"
        app:layout_constraintTop_toTopOf="@+id/menu_container"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/profile_button"
        style="?attr/materialIconButtonStyle"
        android:layout_width="43dp"
        android:layout_height="43dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="14dp"
        android:background="@drawable/profile"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/menu_container"
        app:layout_constraintEnd_toEndOf="@+id/menu_container"
        app:layout_constraintTop_toTopOf="@+id/menu_container"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="377dp"
        android:layout_height="337dp"
        android:dateTextAppearance="@style/TextAppearance.AppCompat.Large"
        android:weekDayTextAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="274dp" />

    <TextView
        android:id="@+id/calendar_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/rubik_bold"
        android:text="Calendar"
        android:textColor="@color/black"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/current_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/rubik_bold"
        android:shadowColor="#000000"
        android:text="19"
        android:textColor="#CA6884"
        android:textSize="90sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="54dp" />

    <TextView
        android:id="@+id/current_day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/rubik_medium"
        android:text="Monday, Sep 2020"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="176dp" />

    <ImageButton
        android:id="@+id/add_trip_cal_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/add_trip"
        tools:layout_editor_absoluteX="299dp"
        tools:layout_editor_absoluteY="24dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="46dp"
        android:layout_height="43dp"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/plus"
        tools:layout_editor_absoluteX="347dp"
        tools:layout_editor_absoluteY="24dp" />

</androidx.constraintlayout.widget.ConstraintLayout>